{
	"manifest_version": 2,
	"name": "InlineEvals",
	"version": "1.0",

	"description": "Displays course evaluation information",
	"icons": {"128": "icon128.png"},

  	"content_scripts": [{
    	"css": ["styles.css"],
    	"js": ["content.js"],
