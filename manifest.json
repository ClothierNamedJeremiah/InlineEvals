{
  "name": "InLineEvals",
  "version": "1.0",
  "description": "Build an Extension!",
  "permissions": ["storage"],
    "content_scripts": [
                        {
                        "matches": ["http://classes.uoregon.edu/pls/prod/*"],
                        "js": ["content.js"]
                        }
                        ],
    "background": {
        "scripts": ["background.js"],
        "persistent": false

    },
  "manifest_version": 2
}
